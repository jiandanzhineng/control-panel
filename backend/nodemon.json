{
  "watch": [
    "index.js",
    "routes",
    "services",
    "utils"
  ],
  "ext": "js,json",
  "ignore": [
    "tests",
    "frontend",
    "node_modules",
    "config/**/*.conf"
  ],
  "exec": "node index.js",
  "env": {
    "NODE_ENV": "development",
    "LOG_LEVEL": "debug"
  },
  "delay": "100ms",
  "legacyWatch": true
}